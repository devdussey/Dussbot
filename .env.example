DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here
# Comma-separated guild IDs for instant dev testing (optional)
GUILD_IDS=your_guild_id_for_testing
# Alternatively, use GUILD_ID for a single test guild
NODE_ENV=production

# Space or comma separated list of bot owner user IDs.
# BOT_OWNER_IDS takes precedence over BOT_OWNER_ID when both are set.
BOT_OWNER_IDS=
# Single bot owner user ID used if BOT_OWNER_IDS is unset.
BOT_OWNER_ID=

# Dashboard Configuration (Optional)
# DASHBOARD_PORT=3000
# DISCORD_CLIENT_SECRET=your_client_secret_here (get from Developer Portal)
# DISCORD_CALLBACK_URL=http://localhost:3000/auth/callback
# SESSION_SECRET=your-session-secret-change-me

# Optional: when set to true, fall back to DMing owners if log channel fails.
# OWNER_FALLBACK_ON_CHANNEL_FAIL=true

# Logging (Optional - use /logconfig setup instead of manual configuration)
# ERROR_LOG_CHANNEL_ID=your_error_log_channel_id
# CONSOLE_RELAY_LEVELS=error,warn

# Image Processing (Optional)
# /removebg uses remove.bg API (requires REMOVE_BG_API_KEY)
# Free tier: 2 uses/day, Premium: unlimited
# Get API key at: https://www.remove.bg/api
# REMOVE_BG_API_KEY=your_remove_bg_key_here
# /highdef uses Hugging Face (optional, requires HF_API_KEY)
# HF_API_KEY=your_huggingface_key_here

# AI Features (Optional)
# /chat, /analysis, /transcribe use OpenAI (requires OPENAI_API_KEY)
# OPENAI_API_KEY=your_openai_key_here
# CHAT_MODEL=gpt-4o-mini

# /summarize uses Hugging Face (requires HF_API_KEY)
# Get free token at: https://huggingface.co/settings/tokens
# HF_API_KEY=your_huggingface_token_here
# HF_SUMMARIZE_MODEL=facebook/bart-large-cnn (default, fast & accurate)

# Premium Features (Optional)
# PREMIUM_USER_IDS=user_id_1 user_id_2
# PREMIUM_GUILD_IDS=guild_id_1 guild_id_2

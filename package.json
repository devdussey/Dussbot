{
  "name": "dussbot",
  "version": "1.5.0",
  "description": "Dussbot Disphoria Clone",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "node scripts/deploy-commands.js && node src/index.js",
    "dev": "node --watch src/index.js",
    "build": "node scripts/deploy-commands.js",
    "test": "node --test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:watch": "tsc -p tsconfig.json --noEmit --watch",
    "build:ts": "tsc -p tsconfig.build.json",
    "start:ts": "npm run build:ts && node scripts/deploy-commands.js && node dist/index.js",
    "dev:ts": "npm run build:ts && node dist/index.js",
    "start:bootstrap": "node bootstrap.js"
  },
  "keywords": [
    "discord",
    "bot",
    "logging",
    "discord.js"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.6.0",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mysql2": "^3.15.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.9.3"
  }
}
